{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "font-size: -webkit-xxx-large",
          "-webkit-tap-highlight-color",
          "-moz-backface-visibility",
          "-webkit-text-size-adjust",
          "text-decoration-skip-ink",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "text-underline-offset",
          "overflow-anchor",
          "min-height: auto"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[allow]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off",
        "duplicate-id-aria": "off"
      }
    ],
    "css-prefix-order": "off",
    "disown-opener": "off",
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not samsung <= 21"
  ]
}